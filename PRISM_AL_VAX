<exec when="init">
def zipCodeFile(fname, zipHeader):
  try:
      zipCodesData = File( "%s/%s" % (gv.survey.path, fname), zipHeader)
  except IOError:
      zipCodesData = ""
  return zipCodesData
</exec>

<suspend/>

<exec when="init">
ZIPCODES1 = zipCodeFile("find_duplicate.dat", "unique")
ZIPCODES2 = zipCodeFile("find_duplicate.dat", "d2")
