      <choice label="ch1" value="5">Most<br />Important</choice>
      <choice label="ch2" value="4">2nd <br />most important</choice>
      <choice label="ch5" value="3">3rd<br />most important</choice>
      <choice label="ch4" value="2">4th<br />most important</choice>
      <choice label="ch6" value="1">Least<br />important</choice>
      <row label="r1"><strong>Oil &amp;amp; Gas</strong></row>
      <row label="r2"><strong>Automotive Manufacturing</strong></row>
